@font-face {
	font-family: "{{fontName}}";
	src: {{{src}}};
	font-weight: normal;
	font-style: normal;
}

{{baseSelector}} {
	display: inline-block;
	font-family: "{{fontName}}";
	font-style: normal;
	font-weight: normal;
	line-height: 1;
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
}

{{#each codepoints}}
{{../baseSelector}}-{{@key}}:before {
	content: "\\{{this}}";
}
{{/each}}


